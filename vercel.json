{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/retreats",
      "has": [
        {
          "type": "host",
          "value": "retreats.nichuzz.com"
        }
      ],
      "permanent": false
    }
  ]
}


